{
    "name": "asseco-voice/laravel-custom-fields",
    "description": "Laravel support for custom fields",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.4",
        "mockery/mockery": "^1.3.1",
        "fzaninotto/faker": "^1.9.1",
        "orchestra/testbench":  "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Asseco\\CustomFields\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asseco\\CustomFields\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\CustomFields\\CustomFieldsServiceProvider"
            ]
        }
    }
}
