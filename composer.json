{
    "name": "asseco-voice/laravel-custom-fields",
    "description": "Laravel support for custom fields",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "asseco-voice/laravel-common": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.4.4",
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^8.5",
        "doctrine/dbal": "^2.10"
    },
    "autoload": {
        "psr-4": {
            "Asseco\\CustomFields\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asseco\\CustomFields\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\CustomFields\\CustomFieldsServiceProvider"
            ]
        }
    }
}
